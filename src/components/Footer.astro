---
import { t, type Lang } from '../i18n/utils';

interface Props {
  lang: Lang;
}

const { lang } = Astro.props;
---

<footer id="contact" class="footer">
    <h2>{t(lang, 'contact.title')}</h2>
    <p>{t(lang, 'contact.address')}</p>
    <p>{t(lang, 'contact.phone')}</p>
    <p>{t(lang, 'contact.email')}</p>
</footer>
